{
  "name": "userapi",
  "version": "1.0.0",
  "description": "A basic User API built with NodeJS, ExpressJS and Sequelize",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "test:app": "cross-env-shell NODE_ENV=test sequelize db:migrate:undo:all && sequelize db:migrate && nyc --reporter=html --reporter=text mocha server/tests/*.test.js --reporter spec --compilers js:babel-register",
    "coverage": "nyc report --reporter=text-loov | coveralls",
    "start": "nodemon index --exec babel-node"
  },
  "author": "Isioye Mohammed",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "pg": "^7.2.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.5.0",
    "sequelize-cli": "^2.8.0",
    "swagger-jsdoc": "^1.9.7",
    "validatorjs": "^3.13.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0",
    "supertest": "^3.0.0"
  }
}
